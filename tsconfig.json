    "outDir": "./dist",
    "allowJs": true,
    "target": "es6",
    "sourceMap": true,
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "strictNullChecks": false
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", ".vscode"]
}
